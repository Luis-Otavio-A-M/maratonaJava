package maratona.javacore.associacao.dominio;

public class Jogador {
	private String nome;

	public Jogador(String name) {
		this.nome = name;
	}

	public String getName() {
		return nome;
	}

	public void setName(String name) {
		this.nome = name;
	}
	
	public void imprimir() {
		System.out.println(nome);
	}
}
